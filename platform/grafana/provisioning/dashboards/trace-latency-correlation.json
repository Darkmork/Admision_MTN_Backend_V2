{
  "dashboard": {
    "id": null,
    "uid": "mtn-trace-latency",
    "title": "MTN Latency & Traces Correlation",
    "description": "Correlation dashboard between HTTP latency metrics and distributed traces",
    "tags": ["mtn", "traces", "latency", "correlation", "jaeger"],
    "style": "dark",
    "timezone": "America/Santiago",
    "editable": false,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "hideControls": false,
    "links": [
      {
        "icon": "external link",
        "tags": [],
        "title": "Jaeger UI",
        "tooltip": "Open Jaeger UI for trace exploration",
        "type": "link",
        "url": "http://jaeger:16686"
      }
    ],
    "liveNow": false,
    "panels": [
      {
        "id": 1,
        "title": "HTTP P99 Latency with Exemplars",
        "type": "timeseries",
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "service:http:p99_latency_seconds{app=~\"$app\"}",
            "legendFormat": "P99 Latency - {{app}}",
            "refId": "A",
            "exemplar": true
          },
          {
            "expr": "service:http:p95_latency_seconds{app=~\"$app\"}",
            "legendFormat": "P95 Latency - {{app}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s",
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/P99.*/"
              },
              "properties": [
                {
                  "id": "custom.lineWidth",
                  "value": 3
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "calcs": ["last", "mean", "max"],
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 2,
        "title": "Distributed Traces - Recent Slow Requests",
        "type": "traces",
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 10
        },
        "targets": [
          {
            "datasource": {
              "type": "jaeger",
              "uid": "jaeger-uid"
            },
            "query": "service=\"$app\" duration>2s",
            "refId": "A"
          }
        ],
        "options": {
          "showTraceId": true,
          "spanBarLabel": "duration"
        }
      },
      {
        "id": 3,
        "title": "Error Rate vs Trace Error Rate",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        },
        "targets": [
          {
            "expr": "service:http:error_rate{app=~\"$app\"}",
            "legendFormat": "HTTP Error Rate - {{app}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "calcs": ["last", "mean"],
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 4,
        "title": "Request Rate & Trace Volume Correlation",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        },
        "targets": [
          {
            "expr": "service:http:rps{app=~\"$app\"}",
            "legendFormat": "HTTP RPS - {{app}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps",
            "min": 0
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "calcs": ["last", "mean", "max"],
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 5,
        "title": "Trace Sampling & Coverage",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 30
        },
        "targets": [
          {
            "datasource": {
              "type": "jaeger",
              "uid": "jaeger-uid"
            },
            "query": "service=\"$app\"",
            "queryType": "metrics",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1000
                },
                {
                  "color": "red",
                  "value": 10000
                }
              ]
            },
            "unit": "short",
            "displayName": "Total Traces"
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value",
          "colorMode": "background"
        }
      },
      {
        "id": 6,
        "title": "Average Span Count per Trace",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 30
        },
        "targets": [
          {
            "expr": "avg(jaeger_traces_spans_total{service=~\"$app\"})",
            "legendFormat": "Avg Spans/Trace",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 20
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            },
            "unit": "short",
            "decimals": 1
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value",
          "colorMode": "background"
        }
      },
      {
        "id": 7,
        "title": "Trace Processing Latency",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 30
        },
        "targets": [
          {
            "expr": "jaeger_agent_jaeger_thrift_udp_t_processor_process_latency_p99{job=\"jaeger\"}",
            "legendFormat": "Processing P99",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            },
            "unit": "s",
            "decimals": 3
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value",
          "colorMode": "background"
        }
      }
    ],
    "refresh": "30s",
    "schemaVersion": 39,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "30s",
        "1m",
        "2m",
        "5m",
        "15m",
        "30m",
        "1h"
      ]
    },
    "templating": {
      "list": [
        {
          "current": {
            "selected": true,
            "text": ["All"],
            "value": ["$__all"]
          },
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "definition": "label_values(http_server_requests_total, app)",
          "hide": 0,
          "includeAll": true,
          "multi": true,
          "name": "app",
          "options": [],
          "query": {
            "query": "label_values(http_server_requests_total, app)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query"
        }
      ]
    },
    "version": 1,
    "weekStart": ""
  }
}