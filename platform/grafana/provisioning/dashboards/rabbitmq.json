{
  "dashboard": {
    "id": null,
    "uid": "mtn-rabbitmq",
    "title": "MTN RabbitMQ Queues",
    "description": "RabbitMQ monitoring dashboard for MTN Admission System message queues",
    "tags": ["mtn", "rabbitmq", "messaging", "queues"],
    "style": "dark",
    "timezone": "America/Santiago",
    "editable": false,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "hideControls": false,
    "links": [
      {
        "icon": "external link",
        "tags": [],
        "title": "RabbitMQ Management",
        "tooltip": "RabbitMQ Management Interface",
        "type": "link",
        "url": "http://rabbitmq:15672"
      }
    ],
    "liveNow": false,
    "panels": [
      {
        "id": 1,
        "title": "Queue Depth (Messages Ready) - SLO Thresholds",
        "type": "bargauge",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "queue:messages:ready",
            "legendFormat": "{{queue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 500
                },
                {
                  "color": "orange",
                  "value": 1000
                },
                {
                  "color": "red",
                  "value": 2000
                }
              ]
            },
            "unit": "short",
            "min": 0,
            "displayName": "${__field.labels.queue}",
            "mappings": []
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/.*dlq.*|.*dead.*/"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "thresholds"
                  }
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": null
                      },
                      {
                        "color": "red",
                        "value": 1
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "displayMode": "basic",
          "valueMode": "color",
          "showUnfilled": true
        }
      },
      {
        "id": 2,
        "title": "Dead Letter Queue (DLQ) Messages - SLO: Must be 0",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "queue:dlq:messages",
            "legendFormat": "{{queue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            },
            "unit": "short",
            "min": 0,
            "displayName": "${__field.labels.queue}",
            "noValue": "0"
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 3,
        "title": "Message Publish Rate (messages/sec)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "queue:messages:publish_rate",
            "legendFormat": "Publish - {{queue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "msgs",
            "min": 0
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "calcs": ["last", "mean", "max"],
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 4,
        "title": "Message Consumption Rate (messages/sec)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "queue:messages:consume_rate",
            "legendFormat": "Consume - {{queue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "msgs",
            "min": 0
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "calcs": ["last", "mean", "max"],
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 5,
        "title": "Queue Processing Balance (Publish vs Consume)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "queue:messages:publish_rate{queue=~\"admissions|evaluations|notifications\"}",
            "legendFormat": "Publish - {{queue}}",
            "refId": "A"
          },
          {
            "expr": "queue:messages:consume_rate{queue=~\"admissions|evaluations|notifications\"}",
            "legendFormat": "Consume - {{queue}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "msgs",
            "min": 0
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/Consume.*/"
              },
              "properties": [
                {
                  "id": "custom.drawStyle",
                  "value": "line"
                },
                {
                  "id": "custom.lineWidth",
                  "value": 2
                }
              ]
            },
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/Publish.*/"
              },
              "properties": [
                {
                  "id": "custom.fillOpacity",
                  "value": 20
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "calcs": ["last", "mean"],
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 6,
        "title": "Consumer Count per Queue",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "rabbitmq_queue_consumers{queue=~\"admissions|evaluations|notifications\"}",
            "legendFormat": "{{queue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 2
                }
              ]
            },
            "unit": "short",
            "min": 0,
            "displayName": "${__field.labels.queue}"
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 7,
        "title": "Unacknowledged Messages",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 24
        },
        "targets": [
          {
            "expr": "queue:messages:unacknowledged",
            "legendFormat": "{{queue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 500
                }
              ]
            },
            "unit": "short",
            "min": 0,
            "displayName": "${__field.labels.queue}"
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 8,
        "title": "RabbitMQ Node Health",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 24
        },
        "targets": [
          {
            "expr": "up{job=\"rabbitmq\"}",
            "legendFormat": "RabbitMQ",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "unit": "short",
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN"
                  },
                  "1": {
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "name",
          "colorMode": "background"
        }
      }
    ],
    "refresh": "30s",
    "schemaVersion": 39,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "30s",
        "1m",
        "2m",
        "5m",
        "15m",
        "30m",
        "1h"
      ]
    },
    "templating": {
      "list": []
    },
    "version": 1,
    "weekStart": ""
  }
}